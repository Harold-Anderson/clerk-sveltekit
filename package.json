{
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "test": "bun test src && playwright test",
    "test:unit": "bun test src",
    "test:e2e": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "files": ["dist"],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "name": "clerk-sveltekit",
  "version": "0.1.0",
  "description": "Adapter to use Clerk with SvelteKit",
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "repository": "git@github.com:markjaquith/clerk-sveltekit.git",
  "author": "Mark Jaquith <mark@jaquith.me>",
  "license": "MIT",
  "peerDependencies": {
    "svelte": "^4.0.0"
  },
  "devDependencies": {
    "@clerk/clerk-js": "^4.60.1",
    "@clerk/clerk-sdk-node": "^4.12.12",
    "@clerk/types": "^3.54.0",
    "@babel/preset-typescript": "^7.18.6",
    "@changesets/cli": "^2.26.2",
    "@playwright/test": "^1.38.1",
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/kit": "^1.25.1",
    "@sveltejs/package": "^2.2.2",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte": "^2.34.0",
    "publint": "^0.2.4",
    "svelte": "^4.0.0",
    "svelte-check": "^3.5.2",
    "tslib": "^2.6.2",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.11"
  }
}
